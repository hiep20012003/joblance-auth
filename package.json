{
  "name": "auth-service",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "dotenv -e .env.prod -- pm2 start ./build/src/app.js -i 5 --attach --watch",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "dev": "dotenv -e .env.dev -- nodemon -r tsconfig-paths/register src/app.ts",
    "link": "npm link @hiep20012003/joblance-shared",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "test": "jest --coverage --maxWorkers=1 --forceExit --detectOpenHandles --watchAll=false",
    "migrate:create": "dotenv -- node scripts/migrate create --folder src/databases/migrations",
    "migrate:up": "dotenv node scripts/migrate.js up",
    "migrate:undo": "dotenv -- node scripts/migrate.js down",
    "migrate:pending": "dotenv -- node scripts/migrate.js pending",
    "seed:up": "dotenv node scripts/seed.js up",
    "seed:create": "dotenv -- node scripts/seed.js create --folder src/databases/seeders",
    "seed:undo": "dotenv -- node scripts/seed.js down"
  },
  "dependencies": {
    "@elastic/opentelemetry-node": "^1.6.0",
    "@faker-js/faker": "^10.1.0",
    "@hiep20012003/joblance-shared": "^0.0.5",
    "amqplib": "^0.10.9",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.2",
    "node-cron": "^4.2.1",
    "pem-jwk": "^2.0.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "tslib": "^2.8.1",
    "umzug": "^3.8.2",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/estree": "^1.0.7",
    "@types/express": "^5.0.3",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.16.3",
    "@types/pem-jwk": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "typescript-transform-paths": "^3.5.3"
  }
}
